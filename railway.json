{
  "builds": [
    {
      "src": "server/package.json",
      "use": "@railway/node"
    },
    {
      "src": "client/package.json",
      "use": "@railway/static"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/server"
    },
    {
      "src": "/(.*)",
      "dest": "/client"
    }
  ]
}